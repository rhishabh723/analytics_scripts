 EVENTMSGACTIONTYPE=4 THEN 'Timeout Official'\n","     WHEN EVENTMSGTYPE IN (9) AND EVENTMSGACTIONTYPE=5 THEN 'Timeout Advance'\n","     WHEN EVENTMSGTYPE IN (9) AND EVENTMSGACTIONTYPE=7 THEN 'Coach Challenge'\n","     WHEN EVENTMSGTYPE IN (10) AND EVENTMSGACTIONTYPE=0 THEN 'Jump ball'\n","     WHEN EVENTMSGTYPE IN (10) AND EVENTMSGACTIONTYPE=1 THEN 'Jump ball CC'\n","     WHEN EVENTMSGTYPE IN (11) AND EVENTMSGACTIONTYPE=0 THEN 'Ejection: No ejection'\n","     WHEN EVENTMSGTYPE IN (11) AND EVENTMSGACTIONTYPE=1 THEN 'Ejection: Second Technical'\n","     WHEN EVENTMSGTYPE IN (11) AND EVENTMSGACTIONTYPE=2 THEN 'Ejection: Second Flagrant Type 1 '\n","     WHEN EVENTMSGTYPE IN (11) AND EVENTMSGACTIONTYPE=3 THEN 'Ejection: First Flagrant Type 2'\n","     WHEN EVENTMSGTYPE IN (11) AND EVENTMSGACTIONTYPE=4 THEN 'Ejection: Other'\n","     WHEN EVENTMSGTYPE IN (12) THEN 'Start of period'\n","     WHEN EVENTMSGTYPE IN (13) THEN 'End of period'\n","     ELSE 'No event'\n","     END as Event_Types,\n","HOMEDESCRIPTION, NEUTRALDESCRIPTION, VISITORDESCRIPTION, SCORE, SCOREMARGIN,\n","CAST(SPLIT(SCORE,'-')[OFFSET(0)] as INT64) VISITOR_SCORE,\n","CAST(SPLIT(SCORE,'-')[OFFSET(1)] as INT64) HOME_SCORE,\n","PLAYER1_NAME, PLAYER2_NAME, PLAYER3_NAME,\n","SPLIT(PCTIMESTRING,':')[OFFSET(0)] MINUTES_REMAINING,\n","SPLIT(PCTIMESTRING,':')[OFFSET(1)] SECONDS_REMAINING,\n","CASE WHEN PLAYER2_NAME IS NOT NULL THEN 1 ELSE 0 END AS SECONDARY_FLAG,\n","CASE WHEN PLAYER2_NAME IS NOT NULL THEN PLAYER2_NAME ELSE NULL END AS SECONDARY_BY,\n","CASE WHEN PLAYER3_NAME IS NOT NULL THEN 1 ELSE 0 END AS TERTIARY_FLAG,\n","CASE WHEN PLAYER3_NAME IS NOT NULL THEN PLAYER3_NAME ELSE NULL END TERTIARY_BY\n","FROM `perceptive-ivy-290216.play_by_play.playoffs_96_97`\n","-- WHERE EVENTMSGTYPE IN (1,2)\n","-- AND GAME_ID='0022000001'\n","ORDER BY 1,2\n",")\n",",\n","\n","shot_chart as (\n","SELECT\n","*\n","FROM\n","`perceptive-ivy-290216.nba_api_shooting_location.playoffs_x_y_location`\n","WHERE Season=\"1996-97\"\n",")\n",",\n","\n","Final as (\n","SELECT\n","A.GAME_ID GAME_ID_PBP, EVENTNUM,\n","A.PERIOD, A.MINUTES_REMAINING, A.SECONDS_REMAINING, SCORE,\n","VISITOR_SCORE, HOME_SCORE, SCOREMARGIN, Event, Event_Types, HOMEDESCRIPTION, NEUTRALDESCRIPTION, VISITORDESCRIPTION,\n","PLAYER1_NAME, PLAYER2_NAME, PLAYER3_NAME,\n","SECONDARY_FLAG,\n","SECONDARY_BY,\n","TERTIARY_FLAG,\n","TERTIARY_BY,\n","B.GAME_EVENT_ID,\n","B.PLAYER_NAME,\n","B.TEAM_NAME,\n","B.SHOT_TYPE,\n","B.SHOT_ZONE_BASIC,\n","B.SHOT_ZONE_AREA,\n","B.SHOT_ZONE_RANGE,\n","B.SHOT_DISTANCE,\n","B.LOC_X,\n","B.LOC_Y,\n","SHOT_ATTEMPTED_FLAG,\n","SHOT_MADE_FLAG,\n","HTM,\n","VTM,\n","Season\n","FROM\n","pbp_data A\n","LEFT JOIN\n","shot_chart B\n","ON A.GAME_ID=B.GAME_ID AND CAST(A.MINUTES_REMAINING AS INT64)=B.MINUTES_REMAINING AND CAST(A.SECONDS_REMAINING AS INT64)=B.SECONDS_REMAINING AND A.PLAYER1_NAME=B.PLAYER_NAME AND A.PERIOD=B.PERIOD\n","-- WHERE A.GAME_ID='0022000001'\n","ORDER BY A.GAME_ID, EVENTNUM\n",")\n",",\n","\n","GAME_RESULTS AS (\n","SELECT GAME_ID ID,TEAM_NAME, MATCHUP, WL, GAME_DATE FROM\n","perceptive-ivy-290216.nba_api_team_boxscore.playoffs_team_box_96_onwards\n","GROUP BY 1,2,3,4,5\n",")\n","\n","SELECT\n","A.*, B.GAME_DATE, B.MATCHUP, B.WL\n","FROM\n","Final A\n","LEFT JOIN\n","GAME_RESULTS B\n","ON A.GAME_ID_PBP=B.ID AND A.TEAM_NAME=B.TEAM_NAME\n","order by GAME_ID_PBP, EVENTNUM\n","\n","\"\"\"\n","project_id = \"perceptive-ivy-290216\"\n","df_bq = pd.read_gbq(query=query, project_id=project_id, dialect='standard')"],"metadata":{"id":"D3P-TZSHIJz5","executionInfo":{"status":"ok","timestamp":1696990265569,"user_tz":360,"elapsed":12509,"user":{"displayName":"Rhishabh Deshpande","userId":"14247620905211190896"}}},"execution_count":412,"outputs":[]},{"cell_type":"code","source":["#Making sure all games are in here\n","df_bq.GAME_ID_PBP.nunique()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"nBsqSC8uJ1Be","executionInfo":{"status":"ok","timestamp":1696990265570,"user_tz":360,"elapsed":25,"user":{"displayName":"Rhishabh Deshpande","userId":"14247620905211190896"}},"outputId":"af18bd09-6f8b-4ae9-88de-e23dc45b69c1"},"execution_count":413,"outputs":[{"output_type":"execute_result","data":{"text/plain":["72"]},"metadata":{},"execution_count":413}]},{"cell_type":"code","source":["game=df_bq"],"metadata":{"id":"Sb-RACUkJ-Ar","executionInfo":{"status":"ok","timestamp":1696990265570,"user_tz":360,"elapsed":20,"user":{"displayName":"Rhishabh Deshpande","userId":"14247620905211190896"}}},"execution_count":414,"outputs":[]},{"cell_type":"code","source":["game=game.replace(\"TIE\",'0')"],"metadata":{"id":"wW2_DpZGpuZc","executionInfo":{"status":"ok","timestamp":1696990265571,"user_tz":360,"elapsed":20,"user":{"displayName":"Rhishabh Deshpande","userId":"14247620905211190896"}}},"execution_count":415,"outputs":[]},{"cell_type":"code","source":["game[\"RK\"] = game.groupby(\"GAME_ID_PBP\")[\"EVENTNUM\"].rank(method=\"dense\", ascending=True)"],"metadata":{"id":"i6hcyzRbRqCu","executionInfo":{"status":"ok","timestamp":1696990265571,"user_tz":360,"elapsed":20,"user":{"displayName":"Rhishabh Deshpande","userId":"14247620905211190896"}}},"execution_count":416,"outputs":[]},{"cell_type":"code","source":["game = game.sort_values(['GAME_ID_PBP','EVENTNUM']).reset_index(drop=True)"],"metadata":{"id":"myTbUQpHDyrF","executionInfo":{"status":"ok","timestamp":1696990265910,"user_tz":360,"elapsed":359,"user":{"displayName":"Rhishabh Deshpande","userId":"14247620905211190896"}}},"execution_count":417,"outputs":[]},{"cell_type":"code","source":["game.sort_values(\"RK\").tail()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":313},"id":"xbQjCI2s1Tvj","executionInfo":{"status":"ok","timestamp":1696990265911,"user_tz":360,"elapsed":6,"user":{"displayName":"Rhishabh Deshpande","userId":"14247620905211190896"}},"outputId":"31b0c56e-5509-486d-881b-ec51c7614138"},"execution_count":418,"outputs":[{"output_type":"stream","name":"stdout","text":["Warning: T